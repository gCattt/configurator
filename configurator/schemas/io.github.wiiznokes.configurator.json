{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Config",
  "description": "Config description",
  "type": "object",
  "properties": {
    "active": {
      "description": "Activate something",
      "default": false,
      "type": "boolean"
    },
    "choice": {
      "default": "A",
      "allOf": [
        {
          "$ref": "#/definitions/Choice"
        }
      ]
    },
    "opt": {
      "default": null,
      "type": [
        "string",
        "null"
      ]
    },
    "otros": {
      "default": 0,
      "type": "integer",
      "format": "uint16",
      "minimum": 0.0
    },
    "sub": {
      "default": {
        "hella": {
          "hella": "bonjour"
        }
      },
      "allOf": [
        {
          "$ref": "#/definitions/SubConfig"
        }
      ]
    },
    "sub_enum": {
      "default": "C",
      "allOf": [
        {
          "$ref": "#/definitions/EnumSubConfig"
        }
      ]
    }
  },
  "definitions": {
    "A": {
      "type": "object",
      "properties": {
        "s": {
          "default": "nested",
          "type": "string"
        }
      }
    },
    "B": {
      "type": "object"
    },
    "Choice": {
      "type": "string",
      "enum": [
        "A",
        "B"
      ]
    },
    "EnumSubConfig": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "C"
          ]
        },
        {
          "type": "object",
          "required": [
            "A"
          ],
          "properties": {
            "A": {
              "$ref": "#/definitions/A"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "B"
          ],
          "properties": {
            "B": {
              "$ref": "#/definitions/B"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "D"
          ],
          "properties": {
            "D": {
              "type": "integer",
              "format": "int32"
            }
          },
          "additionalProperties": false
        }
      ]
    },
    "Hella": {
      "type": "object",
      "properties": {
        "hella": {
          "default": "niquer sa mere",
          "type": "string"
        }
      }
    },
    "SubConfig": {
      "type": "object",
      "properties": {
        "hella": {
          "default": {
            "hella": "bonsoir"
          },
          "allOf": [
            {
              "$ref": "#/definitions/Hella"
            }
          ]
        }
      }
    }
  },
  "X_CONFIG_PATH": "configurator/config/io.github.wiiznokes.configurator.json"
}